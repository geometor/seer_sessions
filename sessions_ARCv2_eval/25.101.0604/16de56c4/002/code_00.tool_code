def count_diff(grid1, grid2):
    diff = 0
    if not grid1 or not grid2 or len(grid1) != len(grid2) or len(grid1[0]) != len(grid2[0]):
        return -1 # Indicates size mismatch or empty grid
    rows = len(grid1)
    cols = len(grid1[0])
    for r in range(rows):
        for c in range(cols):
            if grid1[r][c] != grid2[r][c]:
                diff += 1
    return diff

